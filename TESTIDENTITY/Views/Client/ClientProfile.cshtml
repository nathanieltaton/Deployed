@model TESTIDENTITY.Data.Client;
@{
}

<h1>Client Profile</h1>

<div class="mt-2">
    <label class="form-label">Id</label>
    <input type="text" class="form-control" asp-for="Id" readonly />
</div>
<div class="mt-2">
    <label class="form-label">First Name</label>
    <input type="text" class="form-control" asp-for="FirstName" readonly />
</div>

<div class="mt-2">
    <label class="form-label">Last Name</label>
    <input type="text" id="LastName" class="form-control" asp-for="LastName" readonly />
</div>

<div class="mt-2">
    <label class="form-label">Phone Number</label>
    <input type="text" id="PhoneNumber" class="form-control" asp-for="PhoneNumber" readonly />
</div>

<div class="mt-2">
    <label class="form-label">Email Address</label>
    <input type="text" id="EmailAddress" class="form-control" asp-for="EmailAddress" readonly />
</div>

<div class="mt-2">
    <label class="form-label">Address</label>
    <input type="text" class="form-control" id="Address" asp-for="Address" readonly />
</div>

<div class="mt-2">
    <label class="form-label">Notes</label>
    <input type="text" class="form-control" id="Notes" asp-for="Notes" readonly />
</div>

<div class="mt-2">
    <label class="form-label">Birthday</label>
    <input type="date" class="form-control" id="Birthday" asp-for="Birthday"  readonly/>
</div>
